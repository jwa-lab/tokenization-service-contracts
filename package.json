{
    "name": "@jwalab/tokenization-service-contracts",
    "version": "0.0.10",
    "files": [
        "index.js",
        "dist",
        "typings"
    ],
    "description": "Tokenization Service's Smart Contracts",
    "devDependencies": {
        "@taquito/signer": "^9.1.1",
        "ganache-cli": "^6.12.2",
        "ganache-core": "^2.13.2",
        "husky": "4.3.0",
        "prettier": "2.2.1",
        "truffle": "5.5.0-tezos.4",
        "typescript": "^4.2.4"
    },
    "scripts": {
        "prepublishOnly": "npm run compile && node ./scripts/prepareDist.js && npm run lint-fix && tsc",
        "start-sandbox": "ganache-cli --flavor tezos --seed alice -t $(date -u +\"%Y-%m-%dT%H:%M:%SZ\")",
        "start-florencebox": "docker run --rm --name my-sandbox -e block_time=2 --detach -p 20000:20000 tqtezos/flextesa:20210602 flobox start",
        "lint": "prettier --check .",
        "lint-fix": "prettier --write .",
        "test": "truffle test ./test/warehouse.test.js --network minilab",
        "compile": "truffle compile"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jwa-studios/tokenization-service-contracts.git"
    },
    "keywords": [
        "ligo",
        "tezos",
        "smart",
        "contracts",
        "tokenization",
        "service"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "JWA Lab",
    "license": "Apache-2.0",
    "bugs": {
